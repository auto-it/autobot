{
  "version": 2,
  "alias": "auto-it.now.sh",
  "public": false,
  "builds": [
    {
      "src": "now.js",
      "use": "@now/node"
    },
    {
      "src": "lib/lambdas/*.ts",
      "use": "@now/node"
    },
    {
      "src": "public/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/now.js"
    },
    {
      "src": "/lambda/label",
      "dest": "/lib/lambdas/label.ts"
    },
    {
      "src": "/l/label",
      "dest": "/lib/lambdas/label.ts"
    },
    {
      "src": "/l/octicon/([^/]+)",
      "dest": "/lib/lambdas/octicon.ts?icon=$1"
    }
  ]
}
